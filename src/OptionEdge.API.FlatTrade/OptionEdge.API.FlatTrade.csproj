<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>proftheta</Authors>
    <Company>OptionEdge</Company>
    <Description>.Net client library for FlatTrade REST Api</Description>
    <Copyright>OptionEdge (c) 2022</Copyright>
    <PackageProjectUrl>https://github.com/proftheta/optionedge-api-FlatTrade</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/proftheta/optionedge-api-FlatTrade</RepositoryUrl>
    <AssemblyVersion>1.0.6</AssemblyVersion>
    <Version>$(VersionPrefix)</Version>
	  <VersionPrefix>1.0.6</VersionPrefix>
	  <VersionSuffix></VersionSuffix>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="licenses\LICENSE.txt" Pack="true" PackagePath=""/>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Cryptisk.Utf8Json" Version="1.4.0" />
    <PackageReference Include="LumenWorksCsvReader" Version="4.0.0" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="RestSharp" Version="111.4.0" />
    <PackageReference Include="RestSharp.Serializers.NewtonsoftJson" Version="111.4.0" />
    <PackageReference Include="Websocket.Client" Version="5.1.2" />
  </ItemGroup>
</Project>
